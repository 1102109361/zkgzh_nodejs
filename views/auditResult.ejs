    }

    .tips-title{
        color: #FF0000;
        font-size: 16px;
        font-weight: bold;
    }

    .tips-content{
        margin-top: 5px;
        word-break: break-all;
    }

    .succes-btn{
        display: inline-block;
        padding: 1px 4px;
        color: #44BB00;
        font-size: 12px;
        background: rgba(68, 187, 0, 0.04);
        border: 1px solid rgba(68, 187, 0, 0.4);
        border-radius: 4px;
    }
